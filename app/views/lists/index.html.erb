<h1>All Favorites Lists</h1>

<div>
  <% @lists.each do |list| %>
    <div class="list-item">
      <h3><%= link_to list.name, list_path(list) %></h3>
    </div>
    <div><%= link_to "Edit List", edit_list_path(list) %></div>
  <% end %>

  <h2><%= link_to "+ New List", new_list_path %></h2>
</div>
